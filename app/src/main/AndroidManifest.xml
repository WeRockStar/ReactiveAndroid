<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.werockstar.reactiveandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ReactiveApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.FormActivity" />
        <activity android:name=".view.GithubActivity" />
        <activity android:name=".view.SearchActivity"></activity>
        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="90b26ce4dae81857d3646a606efc117d55b0866e" />
    </application>

</manifest>